{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "demo-tdd-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/demoTdd/DemoTddWebPart.js",
          "manifest": "./src/webparts/demoTdd/DemoTddWebPart.manifest.json"
        }
      ]
    },
    "jotto-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/jotto/JottoWebPart.js",
          "manifest": "./src/webparts/jotto/JottoWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DemoTddWebPartStrings": "lib/webparts/demoTdd/loc/{locale}.js",
    "JottoWebPartStrings": "lib/webparts/jotto/loc/{locale}.js"
  }
}
